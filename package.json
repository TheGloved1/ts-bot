{
  "name": "ts-bot",
  "version": "1.0.0",
  "description": "A simple discord bot using discordx and typescript",
  "main": "build/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:changelog": "bunx @discordx/changelog --src src",
    "dev": "tsx src/main.ts",
    "start": "node build/main.js",
    "build:start": "tsc && node build/main.js",
    "watch": "nodemon --exec bun run dev --watch src --ext ts"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "printWidth": 150
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samarmeena/discord-music-bot.git"
  },
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript"
  ],
  "author": "Vijay Meena <indianoceanroleplay@gmail.com> [https://github.com/samarmeena]",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samarmeena/discord-music-bot/issues"
  },
  "homepage": "https://github.com/samarmeena/discord-music-bot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.14.0",
    "@discordx/importer": "^1.3.1",
    "@discordx/music": "^6.3.1",
    "@discordx/plugin-lava-player": "^2.5.5",
    "@discordx/plugin-ytdl-player": "^2.3.5",
    "@google/generative-ai": "^0.21.0",
    "discord.js": "^14.15.3",
    "discordx": "^11.7.0",
    "dotenv": "^16.4.7",
    "ffmpeg-static": "^5.2.0",
    "isomorphic-unfetch": "^4.0.2",
    "miniget": "^4.2.3",
    "node-gyp": "^11.0.0",
    "node-opus": "^0.3.3",
    "opusscript": "^0.0.8",
    "youtube-sr": "^4.3.11",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.1",
    "nodemon": "^3.1.3",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "5.4.5"
  }
}
